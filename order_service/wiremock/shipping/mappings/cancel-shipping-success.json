{
  "request": {
    "method": "POST",
    "urlPattern": "/api/shipping/cancel/[A-Za-z0-9\\-]+"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": [
      "response-template"
    ],
    "jsonBody": {
      "success": true,
      "trackingNumber": "{{request.path.[2]}}",
      "message": "Shipping successfully cancelled",
      "retryable": false,
      "timestamp": "2025-12-22T22:01:42.746546424"
    }
  }
}