{
  "request": {
    "method": "POST",
    "urlPattern": "/api/payment/refund/[a-zA-Z0-9\\-]+"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": [
      "response-template"
    ],
    "jsonBody": {
      "success": true,
      "transactionId": "{{request.path.[2]}}",
      "message": "Payment successfully refunded",
      "retryable": false,
      "timestamp": "2025-12-22T22:01:42.746546424"
    }
  }
}